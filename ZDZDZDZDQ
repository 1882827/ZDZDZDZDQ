        print("Finished sending shakes. Success: " .. successCount .. ", Failed: " .. failCount)
    else
        warn("Please select a player and enter a valid amount of shakes")
    end
end)

-- Check if remote exists when script starts
task.wait(2)
if not giftRemote then
    warn("WARNING: No gift event was found automatically!")
    print("Please check the output above and manually set the correct event")
end
